{
  "name": "html5module",
  "version": "0.0.1",
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "^1.1.0",
    "@sapui5/ts-types": "1.71.x",
    "@ui5/cli": "^2.6.2",
    "bestzip": "2.1.4",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-qunit": "^4.1.1",
    "karma-sinon": "^1.0.5",
    "karma-ui5": "^2.3.1",
